            "gutterIcon": true,
            "opacity": 90
        },
        "REVISAR": {
            "type": "text",
            "foreground": "#014ba0",
            "background": "#53d4ff",
            "iconColour": "#53d4ff",
            "icon": "info",
            "gutterIcon": true,
            "opacity": 90
        },
        "EXCLUIR": {
            "type": "text",
            "foreground": "#450000",
            "background": "#ec7356",
            "iconColour": "#f00",
